<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background2"
    android:orientation="vertical" >
      
    	<RelativeLayout android:id="@+id/panelIconLeft"
        	android:layout_width="wrap_content"
        	android:layout_height="60dip"
        	android:layout_alignParentLeft="true"
        	android:background="@drawable/menubar">
        
        <TextView
            android:id="@+id/judul"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:paddingLeft="6dp"       
            android:textColor="#FFF"
            android:textSize="18sp"/>

        <Button
            android:id="@+id/back"
            android:layout_width="60dip"
            android:layout_height="40dip"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/next"
            android:text="Kembali"
            android:textColor="#FFF"
            android:textSize="12sp"
            android:typeface="serif" />

 </RelativeLayout>

<ScrollView 
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">
    
		<LinearLayout
  			android:orientation="vertical"
  			android:layout_width="fill_parent"
  			android:layout_height="wrap_content">
  			
		 <LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="fill_parent"
        	android:layout_height="200dip"
        	android:background="#3b3b3b">
  	
        <ImageView     
			android:id="@+id/imageView1"   
	        android:layout_width="170dp"
	        android:layout_height="200dp"
	        android:src="@drawable/no_image"/>	
	            
		<Button
            android:id="@+id/buttontelepon"
            android:layout_width="150dp"
        	android:layout_height="115dp"
            android:background="@drawable/telepon1"
            android:layout_marginTop="40dip"       
            android:textColor="#ffffff"
            android:textStyle="bold" />
	        
        </LinearLayout>
        
        <LinearLayout
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:layout_margin="2dp"
        	android:orientation="vertical" >

        <Button
            android:id="@+id/buttonweb"
            android:layout_width="310dp"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:layout_marginBottom="10dp"
            android:background="#009f00"
            android:drawableLeft="@drawable/website"
            android:text=" LIHAT WEBSITE"
            android:textColor="#ffffff"
            android:textStyle="bold" />
        
    </LinearLayout>
    
  		<TextView android:id="@+id/detail"
            android:layout_width="wrap_content"
            android:textStyle="bold"
            android:textSize="16dip"
            android:paddingLeft="20dip"
            android:paddingTop="5dip"
            android:paddingBottom="5dip"
            android:layout_height="wrap_content"/>
  		
  		 <LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="fill_parent"
        	android:layout_height="fill_parent"
        	android:paddingTop="5dip"
        	android:layout_alignParentLeft="true"
        	android:background="#3b3b3b">
          	
  		<TextView android:id="@+id/content"
      		android:layout_width="wrap_content"
            android:textSize="18dip"
      		android:layout_height="wrap_content"
      		android:paddingLeft="20dip"
      		android:paddingRight="20dip"
      		android:paddingBottom="20dip"
            android:textColor="#ffffff"/>
  		
  		</LinearLayout>
  		
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="wrap_content"
        	android:layout_height="10dp"
        	android:layout_alignParentLeft="true"
        	android:paddingTop="20dip"
        	android:background="@drawable/menubar1">
        
    	</LinearLayout>
    		   		
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:paddingTop="20dip"
        	android:layout_alignParentLeft="true">
        
        <ImageView
            android:id="@+id/home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20dip"
            android:src="@drawable/home"/>
        
        <TextView android:id="@+id/alamat"
      		android:layout_width="wrap_content"          
      		android:layout_height="wrap_content"
      		android:textSize="16dip"
      		android:paddingLeft="20dip"
      		android:layout_gravity="center"
            android:textColor="#333333"/>
        
    	</LinearLayout>
    	
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:paddingTop="10dip"
        	android:layout_alignParentLeft="true">
        
        <ImageView
            android:id="@+id/tlp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20dip"
            android:src="@drawable/tlp"/>
        
        <TextView android:id="@+id/telepon"
      		android:layout_width="wrap_content"          
      		android:layout_height="wrap_content"
      		android:textSize="16dip"
      		android:paddingLeft="20dip"
      		android:layout_gravity="center"
            android:textColor="#333333"/>
        
    	</LinearLayout>
    	
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:paddingTop="10dip"
        	android:layout_alignParentLeft="true">
        
        <ImageView
            android:id="@+id/webicon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20dip"
            android:src="@drawable/website"/>
        
         <TextView android:id="@+id/web"
      		android:layout_width="wrap_content"          
      		android:layout_height="wrap_content"
      		android:textSize="16dip"
      		android:paddingLeft="20dip"
      		android:layout_gravity="center"
            android:textColor="#333333"/>
        
    	</LinearLayout>
    	
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:paddingTop="10dip"
        	android:layout_alignParentLeft="true">
        
        <ImageView
            android:id="@+id/time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20dip"
            android:src="@drawable/time"/>
        
         <TextView android:id="@+id/waktu"
      		android:layout_width="wrap_content"          
      		android:layout_height="wrap_content"
      		android:textSize="16dip"
      		android:paddingLeft="20dip"
      		android:layout_gravity="center"
            android:textColor="#333333"/>
        
    	</LinearLayout>
    	
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:paddingTop="10dip"
        	android:paddingBottom="20dip"
        	android:layout_alignParentLeft="true">
        
        <ImageView
            android:id="@+id/cost"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20dip"
            android:src="@drawable/price"/>
        
         <TextView android:id="@+id/harga"
      		android:layout_width="wrap_content"          
      		android:layout_height="wrap_content"
      		android:textSize="16dip"
      		android:paddingLeft="20dip"
      		android:layout_gravity="center"
            android:textColor="#333333"/>
        
    	</LinearLayout>
  		
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="wrap_content"
        	android:layout_height="10dp"
        	android:layout_alignParentLeft="true"
        	android:background="@drawable/menubar1">
        
    	</LinearLayout>
    	
  		<TextView android:id="@+id/menu"
      		android:layout_width="wrap_content"          
      		android:layout_height="wrap_content"
      		android:textSize="16dip"
      		android:paddingLeft="20dip"
      		android:paddingTop="20dip"
            android:textColor="#333333"
            android:text="aaaaaaaaa"/>
    	
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="fill_parent"
        	android:layout_height="fill_parent"
        	android:layout_marginTop="20dip"
        	android:background="#3b3b3b" >
      
  		<ImageView     
			android:id="@+id/imageView2"   
	        android:layout_width="fill_parent"
	        android:layout_height="300dp"
	        android:layout_gravity="center"
	        android:src="@drawable/no_image"/>
  
 		</LinearLayout>
 		
 		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="fill_parent"
        	android:layout_height="fill_parent"
        	android:layout_marginTop="20dip"
        	android:background="#3b3b3b">
        
  		<ImageView     
			android:id="@+id/imageView3"   
	        android:layout_width="fill_parent"
	        android:layout_height="300dp"
	        android:layout_gravity="center"
	        android:src="@drawable/no_image"/>
  		</LinearLayout>	
 		
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="fill_parent"
        	android:layout_height="fill_parent"
        	android:layout_marginTop="20dip"
        	android:background="#3b3b3b" >
        
  		<ImageView     
			android:id="@+id/imageView4"   
	        android:layout_width="fill_parent"
	        android:layout_height="300dp"
	 		android:layout_gravity="center"
	        android:src="@drawable/no_image"/>
  		 		
 		</LinearLayout>
 		
  		<LinearLayout android:id="@+id/panelIconLeft"
        	android:layout_width="fill_parent"
        	android:layout_height="fill_parent"
        	android:layout_marginTop="20dip"
        	android:background="#3b3b3b" >
      
  		<ImageView     
			android:id="@+id/imageView5"   
	        android:layout_width="fill_parent"
	        android:layout_height="300dp"
	        android:layout_gravity="center"
	        android:src="@drawable/no_image"/>
  
 		</LinearLayout>
 		
</LinearLayout>
</ScrollView>
</LinearLayout>